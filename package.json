{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React Redux Boilerplate for making websites",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrateekAgarwal20/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "framework"
  ],
  "author": "Prateek Agarwal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PrateekAgarwal20/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/PrateekAgarwal20/react-redux-boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "css-loader": "^0.28.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mocha": "^5.0.4",
    "mongoose": "^5.0.10",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.13"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
